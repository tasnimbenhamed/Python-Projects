    "    lst.reverse()\n",
    "    df_perfrisk_s['Y']=lst\n",
    "    df_perfrisk_s['labels']=['<b>'+i+'<br>'+str(round(j,3)) for i,j in zip(df_perfrisk_s['Strategy'],df_perfrisk_s[indicator])]\n",
