    "\n",
    "def get_color(name, number):\n",
    "    pal = list(sns.color_palette(palette=name, n_colors=number).as_hex())\n",
    "    return pal\n",
    "pal_vi = get_color('viridis_r', df_perfrisk.shape[0])\n",
