    "    url_sector=url_stock.replace('cours','societe/profil')\n",
    "    res_sector=requests.get(url_sector,headers=headers)\n",
    "    df_sector=pd.read_html(res_sector.content)\n",
    "    lst_sector.append(df_sector[2][1].loc[0])\n"
   ]
